{
  "name": "@crichmond1989/svn",
  "version": "1.1.0",
  "description": "SVN interface for js",
  "main": "dist/app",
  "typings": "dist/app",
  "scripts": {
    "build": "tsc",
    "lint": "tslint --project .",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testRegex": "/src/.*\\.test\\.ts$"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crichmond1989/svn.git"
  },
  "keywords": [
    "svn"
  ],
  "files": [
    "dist/"
  ],
  "author": "Chris Richmond",
  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {
    "xml2js": "~0.2.8"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/node": "^10.10.1",
    "jest": "^23.6.0",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  }
}