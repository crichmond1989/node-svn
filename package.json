{
  "name": "@crichmond1989/svn",
  "version": "1.3.0",
  "description": "SVN interface for js",
  "main": "dist/app",
  "typings": "dist/app",
  "scripts": {
    "babel": "babel src --out-dir dist --extensions \".ts\"",
    "build": "npm run types && npm run babel",
    "cover": "jest --coverage",
    "lint": "tslint --project .",
    "test": "jest",
    "types": "tsc --emitDeclarationOnly"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testRegex": "/src/.*\\.test\\.ts$",
    "reporters": [
      "default",
      "jest-junit"
    ],
    "coverageReporters": [
      "text",
      "cobertura",
      "html"
    ]
  },
  "jest-junit": {
    "classNameTemplate": "{filename}",
    "output": "./test-results/junit/results.xml",
    "suiteNameTemplate": "{filename}",
    "titleTemplate": "{classname}: {title}"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crichmond1989/svn.git"
  },
  "keywords": [
    "svn"
  ],
  "files": [
    "dist/"
  ],
  "author": "Chris Richmond",
  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {
    "xml2js": "~0.2.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.10.1",
    "babel-preset-minify": "^0.5.0-alpha.3cc09dcf",
    "jest": "^23.6.0",
    "jest-extended": "^0.9.0",
    "jest-junit": "^5.1.0",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  }
}
